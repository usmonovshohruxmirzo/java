// My first program at JAVA - 
package main;

public class HelloWorld {
	public static void main(String[] args) {
		System.err.println("Hello Java");
		System.out.println("-----Hello Java");
		System.out.println("----------Hello Java");
		System.out.println("---------------Hello Java");
		System.out.println("---------------Hello Java");
		System.out.println("----------Hello Java");
		System.out.println("-----Hello Java");
		System.err.println("Hello Java");
	}
}
